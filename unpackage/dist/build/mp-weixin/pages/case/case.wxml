<view class="content"><view class="head"><text>管理</text><image src="../../static/waste.png" mode></image></view><view class="body"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="id"><view class="_ul"><view class="_li"><view class="left"><u-checkbox-group vue-id="{{'fde2a280-1-'+index}}" placement="row" iconPlacement="right" size="25" label="{{item.class}}" name="{{item.class}}" bind:__l="__l" vue-slots="{{['default']}}"><u-checkbox vue-id="{{('fde2a280-2-'+index)+','+('fde2a280-1-'+index)}}" bind:__l="__l"></u-checkbox><view class="left-right"><image src="{{item.src}}" mode></image></view></u-checkbox-group></view><view class="right"><text class="title">{{item.class}}</text><view class="body"><u-count-down vue-id="{{'fde2a280-3-'+index}}" time="{{30*60*60*1000}}" format="HH:mm:ss" bind:__l="__l"></u-count-down></view><view class="foo"><text>{{"￥"+item.money1}}</text><u-number-box bind:input="__e" vue-id="{{'fde2a280-4-'+index}}" value="{{value}}" data-event-opts="{{[['^input',[['__set_model',['','value','$event',[]]]]]]}}" bind:__l="__l"></u-number-box></view></view></view></view></block></view><view class="footer"><view class="footer-left"><u-checkbox-group vue-id="fde2a280-5" placement="row" iconPlacement="right" size="25" bind:__l="__l" vue-slots="{{['default']}}"><u-checkbox vue-id="{{('fde2a280-6')+','+('fde2a280-5')}}" shape="circle" bind:__l="__l"></u-checkbox><text>全选</text></u-checkbox-group></view><view class="footer-right"><text class="title">合计：</text><text class="money">{{"￥"+money}}</text><text style="margin-left:10rpx;">(<text>{{piece}}</text>件）</text></view></view><view class="foot"><view data-event-opts="{{[['tap',[['cancel',['$event']]]]]}}" class="left" bindtap="__e">取消</view><view data-event-opts="{{[['tap',[['payment',['$event']]]]]}}" class="right" bindtap="__e">支付</view></view></view>