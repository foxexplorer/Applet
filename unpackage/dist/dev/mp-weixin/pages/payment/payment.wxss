.content {
  margin: 0;
  padding: 0;
  background-color: #ECF0F2;
}
.ress {
  width: 100%;
  margin: 0 auto;
}
.ress .have {
  width: 100%;
  height: 150rpx;
  display: flex;
  line-height: 150rpx;
  background-color: #fff;
  text-align: center;
}
.ress .have .have_one {
  width: 30%;
  height: 100%;
}
.ress .have .have_two {
  width: 65%;
  height: 100%;
  font-size: 40rpx;
}
.box {
  width: 100%;
  background-color: #fff;
  height: 400rpx;
  margin-top: 20rpx;
  padding-top: 20rpx;
}
.box .head_box {
  width: 90%;
  height: 300rpx;
  margin: 30rpx auto;
  display: flex;
  justify-content: space-between;
  border: 1rpx solid #ccc;
  padding: 20rpx;
}
.box .head_box .head_left {
  width: 40%;
  height: 300rpx;
  background-color: #fff;
}
.box .head_box .head_left image {
  width: 100%;
  height: 100%;
}
.box .head_box .head_right {
  width: 57%;
  height: 300rpx;
}
.box .head_box .head_right ._h3 {
  width: 100%;
  height: 70rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.box .head_box .head_right .body {
  display: flex;
  height: 70rpx;
  line-height: 70rpx;
  justify-content: space-between;
}
.box .head_box .head_right .body .one {
  font-size: 50rpx;
  color: red;
  font-weight: 600;
}
.box .head_box .head_right .body .two {
  color: #ccc;
}
.box .head_box .head_right .bottom {
  display: flex;
  height: 80rpx;
  line-height: 70rpx;
  text-align: center;
  margin-top: 50rpx;
  justify-content: space-between;
}
.box .head_box .head_right .bottom .one {
  border: 1rpx solid #ccc;
  width: 45%;
  height: 100%;
  border-radius: 40rpx;
  color: #ccc;
}
.box .head_box .head_right .bottom .two {
  border: 1rpx solid #ccc;
  width: 45%;
  height: 100%;
  border-radius: 40rpx;
  color: #ccc;
}
.body_box {
  width: 100%;
  margin: 20rpx auto;
  height: 100rpx;
  display: flex;
  justify-content: space-around;
  line-height: 100rpx;
  font-size: 50rpx;
  font-weight: 600;
  background-color: #fff;
}
.body_box .body_box1 {
  height: 100rpx;
}
.body_box .body_box1 .right {
  color: red;
}
.body_box .body_box2 {
  margin-top: 30rpx;
}
.list {
  width: 100%;
  margin: 0 auto;
  background-color: #fff;
}
.list ._ul {
  margin: 0;
  padding: 40rpx;
  list-style: none;
}
.list ._ul ._li u-radio-group {
  margin-top: 30rpx;
}
.foot {
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 120rpx;
  background-color: #fff;
  display: flex;
  justify-content: center;
  text-align: center;
  color: #fff;
}
.foot .middle {
  width: 80%;
  height: 80rpx;
  border-radius: 50rpx;
  line-height: 80rpx;
  margin-top: 20rpx;
  margin-left: 20rpx;
  background-color: red;
}

