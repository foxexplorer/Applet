.content {
  width: 100%;
  height: 100vh;
  background-color: #ECF0F2;
}
.header {
  width: 100%;
  height: 250rpx;
  background-color: #ffb5a6;
}
.header .head_left {
  margin: 60rpx 0 0 30rpx;
  width: 60%;
  height: 60%;
  overflow: hidden;
  float: left;
}
.header .head_left .left-image {
  width: 35%;
  height: 100%;
  float: left;
}
.header .head_left .left-image image {
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.header .head_left .left-text {
  margin: 30rpx 0 0 20rpx;
  float: left;
  line-height: 50rpx;
}
.header .head_left .left-text .text1 {
  font-weight: 600;
  font-size: 35rpx;
}
.header .head_right {
  width: 26%;
  height: 15%;
  background-color: #fff;
  float: left;
  margin: 110rpx 0 0 0;
  padding: 10rpx 5rpx;
  border-radius: 40rpx;
}
.header .head_right .right-text {
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
}
.header .head_right .right-text image {
  width: 20%;
  height: 100%;
}
.header .head_right .right-text text {
  font-size: 33rpx;
  padding-left: 20rpx;
  font-weight: 600;
}
.list {
  position: relative;
  width: 100%;
  height: 260rpx;
  background-color: #ffb5a6;
  color: #fff;
}
.list ._ul {
  margin: 0;
  padding: 0;
  width: 100%;
  list-style: none;
}
.list ._ul ._li {
  width: 25%;
  text-align: center;
  float: left;
  font-size: 40rpx;
}
.list .member {
  position: absolute;
  bottom: 0rpx;
  left: 20rpx;
  width: 95%;
  height: 120rpx;
  margin: 0 auto;
  margin-bottom: 0;
  display: flex;
  justify-content: space-between;
  line-height: 120rpx;
  border-top-left-radius: 40rpx;
  border-top-right-radius: 40rpx;
  background-color: #383838;
}
.list .member .member-left {
  width: 70%;
  border-top-left-radius: 40rpx;
  text-align: center;
  display: flex;
  justify-content: space-around;
  background-image: -webkit-linear-gradient(top, #fff, #f8ff6e);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
}
.list .member .member-left .left-text1 {
  font-size: 40rpx;
}
.list .member .member-left .left-text2 {
  font-size: 30rpx;
}
.list .member .member-right {
  width: 30%;
  border-top-right-radius: 40rpx;
  text-align: center;
}
.list .member .member-right text {
  width: 70%;
  height: 90%;
  padding: 10rpx 25rpx;
  background-color: #fff6ab;
  color: #000;
  font-weight: 600;
  border-radius: 40rpx;
}
.we_box {
  width: 100%;
  height: 300rpx;
  background-color: #fff;
}
.we_box .title {
  width: 95%;
  margin: 0 auto;
  font-size: 40rpx;
  font-weight: 600;
  padding: 35rpx 0;
}
.we_box ._ul {
  width: 90%;
  margin: 0;
  padding: 0;
  list-style: none;
  margin: 0 auto;
  background-color: #30d9ff;
}
.we_box ._ul ._li {
  width: 33%;
  float: left;
  text-align: center;
}
.we_box ._ul ._li image {
  width: 75rpx;
  height: 80rpx;
}
.we2_box {
  margin-top: 30rpx;
  height: 500rpx;
}
.we2_box ._ul ._li {
  margin-bottom: 50rpx;
}
.show {
  padding: 20rpx 20rpx;
  height: 280rpx;
}
.show text {
  font-size: 40rpx;
}
.show .show_content {
  margin: 30rpx auto;
  margin-bottom: 0;
  width: 95%;
}

