.content {
  background-color: #ECF0F2;
  padding-top: calc(var(-–window-bottom)+20rpx);
}
.box {
  width: 100%;
  height: 120rpx;
  border-bottom-left-radius: 30%;
  border-bottom-right-radius: 30%;
  padding: 10rpx;
  box-sizing: border-box;
}
.swiper_box {
  width: 100%;
  height: 400rpx;
  margin: 0 auto;
  overflow: hidden;
}
.swiper_box u-swiper {
  width: 100%;
  height: 100%;
}
.nav_box {
  display: flex;
  justify-content: space-between;
  width: 94%;
  height: 230rpx;
  margin: 0 auto;
  padding: 10rpx;
  box-sizing: border-box;
}
.nav_box image {
  width: 50%;
  height: 100%;
  float: left;
  cursor: pointer;
}
.Navigation {
  width: 95%;
  height: 300rpx;
  margin: 20rpx auto;
  margin-bottom: 0;
}
.Navigation .Nav .Nav-one {
  width: 150rpx;
  height: 150rpx;
  background-color: #fff;
  border-radius: 50%;
  margin: 0 auto;
  margin-top: 25rpx;
  margin-right: 40rpx;
  text-align: center;
}
.Navigation .Nav .Nav-one image {
  margin-top: 25rpx;
  width: 100rpx;
  height: 100rpx;
}
.Navigation .Nav .Nav-two {
  width: 150rpx;
  text-align: center;
  padding-top: 10rpx;
  color: #6b6b6b;
}
.list_box {
  width: 100%;
  padding-top: 20rpx;
  margin-top: 40rpx;
  box-sizing: border-box;
  background-color: #fff;
}
.list_box .titbar {
  width: 95%;
  margin: 0 auto;
}
.list_box .titbar ._h2 {
  display: inline-block;
}
.list_box .titbar .more {
  float: right;
  color: #83a4ad;
}
.list_box .titbar .more image {
  width: 20rpx;
  height: 20rpx;
}
.list_box .list2 {
  width: 98%;
  margin: 30rpx auto;
  margin-bottom: 0rpx;
  padding-bottom: 30rpx;
}
.list_box .list2 .footer {
  width: 100%;
  text-align: center;
}
.list_box .list2 .scroll-view-item {
  width: 100%;
}
.list_box .list2 .scroll-view-item ._ul {
  width: 100%;
  margin: 0;
  padding: 0;
}
.list_box .list2 .scroll-view-item ._ul ._li {
  float: left;
  padding-top: 40rpx;
  background-color: #fff;
  width: 47.5%;
  height: 500rpx;
  cursor: pointer;
  list-style: none;
  padding: 0 8rpx;
}
.list_box .list2 .scroll-view-item ._ul ._li .pic-box {
  width: 100%;
  height: 300rpx;
}
.list_box .list2 .scroll-view-item ._ul ._li .pic-box image {
  width: 100%;
  height: 100%;
  border-radius: 5%;
}
.list_box .list2 .scroll-view-item ._ul ._li .txt-box {
  padding: 10px 10px;
  width: 100%;
  font-size: 30rpx;
}
.list_box .list2 .scroll-view-item ._ul ._li .txt-box .left {
  width: 90%;
}
.list_box .list2 .scroll-view-item ._ul ._li .txt-box .left .span1 {
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.list_box .list2 .scroll-view-item ._ul ._li .txt-box .left .span2 {
  color: red;
}
.list_box .list2 .scroll-view-item ._ul ._li .txt-box .right {
  height: 75rpx;
  line-height: 75rpx;
  width: 50%;
}
.list_box .list2 .scroll-view-item ._ul ._li .txt-box .right .btn {
  padding: 5rpx 15rpx;
  background-color: #80c4ae;
  border-radius: 4px;
  line-height: 30px;
  color: #fff;
  margin-top: 10rpx;
  text-align: center;
  text-decoration: none;
}

